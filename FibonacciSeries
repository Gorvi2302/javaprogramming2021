import java.util.Scanner;

public class Challenge {
    public static void main (String[] args){
        int a = 0, b = 1 , c;
        Scanner sc = new Scanner (System.in);
        System.out.println("How many numbers do you ant to print in the fibonacci series:");
        int num = sc.nextInt();
        System.out.print(a+" "+ b);
        for ( int i = 1; i<=num;i++){
            c = a + b;
            System.out.print(" "+ c);
            a = b;
            b = c;
        }
    }
           
}

